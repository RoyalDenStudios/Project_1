            <string>//Looking at Mouse
    //Down Right
if (mouse_x &gt; Obj_Player.x) &amp;&amp; (mouse_y &gt; Obj_Player.y)
    { sprite_index = Spr_Player_Walk_DR
    image_xscale = 1
    }
    //Up Right
if (mouse_x &gt; Obj_Player.x) &amp;&amp; (mouse_y &lt; Obj_Player.y)
    { sprite_index = Spr_Player_Walk_UR
    image_xscale = 1
    }
    //Up Left
if (mouse_x &lt; Obj_Player.x) &amp;&amp; (mouse_y &lt; Obj_Player.y)
    { sprite_index = Spr_Player_Walk_UR
    image_xscale = -1
    }
    //Down Left
if (mouse_x &lt; Obj_Player.x) &amp;&amp; (mouse_y &gt; Obj_Player.y)
    { sprite_index = Spr_Player_Walk_DR
    image_xscale = -1
    }
//Movement
    //Move Down
if (keyboard_check(ord("S"))) &amp;&amp; (place_free(x,y + 2))
    { y += 2
    image_speed = 0.2
    }
    //Move Up
if (keyboard_check(ord("W"))) &amp;&amp; (place_free(x,y - 2))
    { y -= 2
    image_speed = 0.2
    }
    //Move Left
if (keyboard_check(ord("A"))) &amp;&amp; (place_free(x - 2,y))
    { x -= 2
    image_speed = 0.2
    }
    //Move Right
if (keyboard_check(ord("D"))) &amp;&amp; (place_free(x + 2,y))
    { x += 2
    image_speed = 0.2
    }
//Idle Animation
if (!keyboard_check(ord("D"))) &amp;&amp; (!keyboard_check(ord("A"))) &amp;&amp; (!keyboard_check(ord("S"))) &amp;&amp; (!keyboard_check(ord("W")))
    { image_speed = 0
    image_index = 0
    }
